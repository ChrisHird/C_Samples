#!/QOpenSys/usr/bin/sh
export PWD=$(pwd)
system -kpieb "CRTSRCPF FILE(OSSILE/RTVDIRSZ) RCDLEN(150)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/rtvdirsz.cmd') TOMBR('/QSYS.lib/OSSILE.lib/RTVDIRSZ.file/RTVDIRSZ.mbr') MBROPT(*REPLACE)"
system -kpieb "CRTCMOD MODULE(OSSILE/RTVDIRSZ) SRCSTMF('$PWD/rtvdirsz.c') REPLACE(*YES)"
system -kpieb "CRTPGM PGM(OSSILE/RTVDIRSZ) MODULE(OSSILE/RTVDIRSZ)"
system -kpieb "CRTCMD CMD(OSSILE/RTVDIRSZ) PGM(OSSILE/RTVDIRSZ) SRCFILE(OSSILE/RTVDIRSZ)"
